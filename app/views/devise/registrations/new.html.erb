<%= render 'shared/errors', obj: resource %>
<div class="container-fluid">
  <div class="row">
  <div class="col-xs-12 col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1">

<div class="panel panel-default devise-bs">
  <div class="panel-heading">
    <h4><%= t('.sign_up', default: 'Sign up') %></h4>
  </div>
  <div class="panel-body">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { role: 'form' }) do |f| %>




    <div class="form-group">
      <div class="control-label col-sm-2 col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <%= f.label :first_name %>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <%= f.text_field :first_name, class: "form-control", placeholder: "Enter your first name", autofocus: true %>
      </div>
    </div>



    <div class="form-group">
      <div class="control-label col-sm-2 col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <%= f.label :last_name %>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <%= f.text_field :last_name, class: "form-control", placeholder: "Enter your last name", autofocus: false %>
      </div>
    </div>




      <div class="form-group">
        <div class="control-label col-sm-2 col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
        <%= f.label :email %>
      </div>
      <div class="col-sm-12 col-md-10 col-md-offset-1">
        <%= f.email_field :email, autofocus: false, class: 'form-control' %>
      </div>
          </div>



      <div class="form-group">
        <div class="control-label col-sm-2 col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
          <%= f.label :password %>
        </div>

        <div class="col-sm-12 col-md-10 col-md-offset-1">
          <%= f.password_field :password, class: 'form-control' %>
        </div>
      </div>

   <div class="form-group">
        <div class="control-label col-sm-2 col-xs-12 col-sm-12 col-md-10 col-md-offset-1">
          <%= f.label :password_confirmation %>
        </div>

        <div class="col-sm-12 col-md-10 col-md-offset-1">
          <%= f.password_field :password_confirmation, class: 'form-control' %>
        </div>
      </div>


      <%= f.submit t('.sign_up', default: 'Sign up'), class: 'btn btn-primary' %>
    </div>
  </div>
    <% end %>
  </div>
  </div>

<%= render 'devise/shared/links' %>
